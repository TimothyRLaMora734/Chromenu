{
  "name": "chromenu",
  "productName": "Chromenu",
  "version": "1.0.0",
  "description": "Mobile Chromium in your menu bar",
  "main": "main/index.js",
  "bin": {
    "chromenu": "./bin/cli.js"
  },
  "scripts": {
    "build": "tsc --pretty -p .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "tslint": "tslint $(git ls-files | grep -E \"\\.tsx?$\")",
    "stylelint": "stylelint renderer/style.css",
    "lint": "npm-run-all --parallel tslint stylelint",
    "watch": "guard --watchdir main renderer bin",
    "debug": "NODE_ENV=development electron .",
    "start": "NODE_ENV=production electron .",
    "preversion": "npm run lint",
    "preuninstall": "node ./bin/cli.js --unsetup-launchctl",
    "release": "npm run lint && ./scripts/make-release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/Chromenu.git"
  },
  "keywords": [
    "browser",
    "electron",
    "manubar"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/Chromenu/issues"
  },
  "homepage": "https://github.com/rhysd/Chromenu#readme",
  "devDependencies": {
    "@types/cheerio": "^0.17.31",
    "@types/electron": "^1.4.27",
    "@types/loglevel": "^1.4.29",
    "@types/mousetrap": "^1.5.32",
    "@types/node": "^6.0.48",
    "@types/react": "^0.14.49",
    "@types/react-addons-shallow-compare": "^0.14.17",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.32",
    "@types/redux": "^3.6.31",
    "@types/request": "0.0.33",
    "electron-packager": "^8.3.0",
    "npm-run-all": "^3.1.1",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^14.0.0",
    "tslint": "^3.15.1",
    "tslint-react": "^1.1.0",
    "typescript": "^2.0.10"
  },
  "dependencies": {
    "bulma": "^0.2.3",
    "cheerio": "^0.22.0",
    "electron": "^1.4.7",
    "electron-in-page-search": "^1.1.2",
    "font-awesome": "^4.7.0",
    "immutable": "^3.8.1",
    "loglevel": "^1.4.1",
    "menubar": "github:rhysd/menubar#ensure-to-launch-app",
    "mousetrap": "^1.6.0",
    "page-icon": "^0.3.0",
    "react": "^15.4.0",
    "react-addons-shallow-compare": "^15.4.0",
    "react-bytesize-icons": "github:rhysd/react-bytesize-icons#package",
    "react-dom": "^15.4.0",
    "react-redux": "^4.4.6",
    "redux": "^3.6.0",
    "request": "^2.78.0"
  }
}
