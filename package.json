{
  "name": "chromenu",
  "productName": "Chromenu",
  "version": "0.0.0",
  "description": "Mobile Chromium in your menu bar",
  "main": "main/index.js",
  "scripts": {
    "build:main": "tsc --pretty -p main/",
    "build:renderer": "tsc --pretty -p renderer/",
    "build": "npm run build:main && npm run build:renderer",
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "tslint $(git ls-files | grep -E \"\\.tsx?$\") && stylelint renderer/style.css",
    "watch": "guard --watchdir main renderer",
    "debug": "NODE_ENV=development electron .",
    "start": "NODE_ENV=production electron ."
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/rhysd/Chromenu.git"
  },
  "keywords": [
    "browser",
    "electron",
    "manubar"
  ],
  "author": "rhysd <lin90162@yahoo.co.jp>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/rhysd/Chromenu/issues"
  },
  "homepage": "https://github.com/rhysd/Chromenu#readme",
  "devDependencies": {
    "@types/cheerio": "^0.17.31",
    "@types/electron": "^1.4.24",
    "@types/loglevel": "^1.4.29",
    "@types/node": "^6.0.45",
    "@types/react": "^0.14.41",
    "@types/react-dom": "^0.14.18",
    "@types/react-redux": "^4.4.32",
    "@types/redux": "^3.6.31",
    "@types/request": "0.0.32",
    "stylelint": "^7.5.0",
    "stylelint-config-standard": "^13.0.2",
    "typescript": "^2.0.3"
  },
  "dependencies": {
    "bulma": "^0.2.3",
    "cheerio": "^0.22.0",
    "electron": "^1.4.4",
    "font-awesome": "^4.6.3",
    "immutable": "^3.8.1",
    "loglevel": "^1.4.1",
    "menubar": "github:rhysd/menubar#ensure-to-launch-app",
    "page-icon": "^0.3.0",
    "react": "^15.3.2",
    "react-bytesize-icons": "^0.7.0",
    "react-dom": "^15.3.2",
    "react-redux": "^4.4.5",
    "redux": "^3.6.0",
    "request": "^2.75.0"
  }
}
